    "tslint": "^5.10.0",
    "tslint-consistent-codestyle": "^1.13.0",
    "tslint-eslint-rules": "^5.2.0"
  },
  "scripts": {
    "compile": "tsc && tslint -p . && node bin/buildOrmConfig",
    "test": "nyc -r html mocha test/index",
    "test-cov": "nyc -r lcovonly -r html mocha test/index",
    "postinstall": "tsc",
    "pretest": "tsc&& tslint -p .",
    "dev": "supervisor -i coverage bin/run | pino",
    "start": "node ./bin/run",
    "lint": "tslint -p .",
    "test-nocompile": "nyc -r html mocha test/index",
    "db:migrate": "node bin/buildOrmConfig && typeorm migrations:run",
    "db:revert": "node bin/buildOrmConfig && typeorm migrations:revert",
    "build:bin": "cat entry.js | nexe -o sandra"
  },
  "greenkeeper": {
    "ignore": []
  }
